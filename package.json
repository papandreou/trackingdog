{
  "name": "trackingdog",
  "version": "1.10.0",
  "description": "cli for finding the original source location of a line+column in a generated file, utilizing the source map",
  "main": "lib/TrackingDog.js",
  "scripts": {
    "test": "mocha",
    "coverage": "NODE_ENV=development nyc --reporter=lcov --reporter=text --all -- npm test && echo google-chrome coverage/lcov-report/index.html",
    "lint": "eslint . && prettier --check '**/*.js'",
    "ci": "npm run test && npm run lint && npm run coverage"
  },
  "bin": {
    "trackingdog": "lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papandreou/trackingdog.git"
  },
  "author": "Andreas Lind <andreaslindpetersen@gmail.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@gustavnikolaj/async-main-wrap": "^3.0.0",
    "assetgraph": "^6.0.0",
    "error-stack-parser": "^2.0.1",
    "get-stdin": "^7.0.0",
    "magicpen": "^6.0.2",
    "magicpen-prism": "^4.0.0",
    "nyc": "^15.0.0",
    "urltools": "^0.4.1",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "httpception": "^3.0.0",
    "mocha": "^6.2.0",
    "prettier": "~1.19.1",
    "sinon": "^8.0.1",
    "unexpected": "^11.1.0",
    "unexpected-sinon": "^10.11.1"
  },
  "files": [
    "lib"
  ],
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
}
